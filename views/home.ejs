<%- include("partials/header") %>
<h1>Home</h1>

<p><%= homeStartingContent %></p>
<% if (data.length != 0) { %> <% data.forEach(post => { %>
<h1><%= post.title %></h1>
<p>
  <%= post.content.substring(0,100).concat("....") %>
  <a href="posts/<%= post._id %>">Read More</a>
</p>

<% }) %> <% } %> <%- include("partials/footer") %>
